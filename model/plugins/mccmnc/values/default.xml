<?xml version='1.0' encoding='UTF-8'?>
<resource>

<string name="summary">Query or modify the value of MCC/MNC.</string>
<string name="help"><![CDATA[Usage:

  %s $ mccmnc [mcc=xxx] [mnc=xxx]

  Example:
    - mccmnc
      Original MCC/MNC: 466/92
    - mccmnc mcc=320 mnc=310
      Original MCC/MNC: 466/92
      Updated MCC/MNC: 320/310]]></string>
<string name="read_error">Error! Can't read MCC/MNC correctly!</string>
<string name="update_error">Error! Can't update MCC/MNC successfully!</string>
<string name="original">Original MCC/MNC: %s/%s</string>
<string name="updated">Updated MCC/MNC: %s/%s</string>
<string name="invalid_mcc">Invalid length of MCC!</string>
<string name="invalid_mnc">Invalid length of MNC!</string>
</resource>