<?xml version='1.0' encoding='UTF-8'?>
<usim_modifier>
    <configuration>
        <!-- enable logging -->
        <log>1</log>
        <!-- enable localized language, zero for force English -->
        <localized>1</localized>
        <!-- auto pin cache -->
        <pin>1</pin>
        <!-- auto adm cache -->
        <adm>1</adm>
    </configuration>
